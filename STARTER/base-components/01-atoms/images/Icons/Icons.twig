<div class="IconsDemo" id="icons">
  {% for item in items %}
    <div class="IconsDemo-item">
      {% include '@atoms/images/Icons/Icon.twig' with {
        icon_name: item.value,
        icon_role: item.role,
        icon_title: item.title,
        icon_desc: item.desc,
        icon_decorative: item.decorative,
      } %}
      <pre>{{ item.value }}</pre>
    </div>
  {% endfor %}
</div>
<br>
