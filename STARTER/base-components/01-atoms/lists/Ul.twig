{#
/**
 * Available variables:
 * - ul__base_class - the base classname
 * - ul__modifiers - array of modifiers to add to the base classname
 * - ul__blockname - blockname prepended to the base classname
 *
 * - listItems - TBD
 */
#}
{% set ul__base_class = ul__base_class|default('Ul') %}

<ul {{ bem(ul__base_class, ul__modifiers, ul__blockname) }}>
  {% block list_content %}
    {% for ul__item in ul__items %}
      {% include "@atoms/lists/_ListItem.twig" with {
        list_item_label: ul__item.label,
        list_item_content: ul__item.content,
        li_base_class: ul__item.li_base_class,
        li_base_class: ul__item.li_base_class,
        li_modifiers: ul__item.li_modifiers,
        li_blockname: ul__item.li_blockname,
      } %}
    {% endfor %}
  {% endblock %}
</ul>
